// src/index.js

export default {
  async fetch(request) {
    // 直接返回我们定义好的 HTML 内容
    // 设置 headers 告诉浏览器这是一个 HTML 文件
    return new Response(html, {
      headers: {
        'Content-Type': 'text/html;charset=UTF-8',
      },
    });
  },
};

